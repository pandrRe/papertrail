{
  "name": "pt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "client:dev": "vite --port 3000",
    "client:start": "vite --port 3000",
    "client:build": "vite build && tsc",
    "client:serve": "vite preview",
    "client:test": "vitest run",
    "server:dev": "bun --env-file=\".env.server.local\" --watch server/index.ts",
    "server:start": "bun --env-file=\".env.server\" server/index.ts",
    "server:build:linux": "bun build --compile --minify-whitespace --minify-syntax --target=bun-linux-x64 ./server/index.ts --outfile server-app"
  },
  "dependencies": {
    "@duckdb/node-api": "^1.3.2-alpha.26",
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/eden": "^1.3.3",
    "@google/genai": "^1.16.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.131.5",
    "@tanstack/router-plugin": "^1.121.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "elysia": "^1.3.20",
    "lmdb": "^3.4.2",
    "lucide-react": "^0.476.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/bun": "^1.2.21",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  }
}
